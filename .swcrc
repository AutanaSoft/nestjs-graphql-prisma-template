{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"],
      "@core/*": ["src/core/*"],
      "@generated/*": ["src/core/generated/*"]
    }
  },
  "minify": false
}
