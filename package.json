{
  "name": "nestjs-graphql-prisma-template",
  "version": "1.0.0",
  "description": "Template for Nestjs services with Fastify, SWC, GraphQL and Prisma",
  "author": "admin@autanasoft.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AutanaSoft/nestjs-graphql-prisma-template"
  },
  "homepage": "https://github.com/AutanaSoft/nestjs-graphql-prisma-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AutanaSoft/nestjs-graphql-prisma-template"
  },
  "keywords": [
    "typescript",
    "nestjs",
    "template"
  ],
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --inspect=0.0.0.0:9229 --respawn src/main.ts",
    "build": "npm run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "test": "rimraf coverage .nyc_output && concurrently 'npm:test:unit' 'npm:test:e2e' && npm run coverage:merge",
    "test:unit": "jest tests/src --coverageDirectory coverage/unit",
    "test:e2e": "jest tests/e2e --coverageDirectory coverage/e2e",
    "coverage:merge": "ts-node scripts/merge-coverage.ts",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "build:clean": "rimraf dist; exit 0"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@as-integrations/fastify": "^2.1.1",
    "@fastify/cookie": "^9.3.1",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.8.1",
    "prisma-graphql-type-decimal": "^3.0.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.5.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.105",
    "@swc/jest": "^0.2.30",
    "@types/bcrypt": "^5.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.11.5",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nyc": "^15.1.0",
    "prettier": "^3.2.4",
    "prisma-nestjs-graphql": "^20.0.2",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
